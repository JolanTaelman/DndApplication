<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             tools:context=".Fragments.RegisterFragment">
    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        <TextView
                android:layout_width="264dp"
                android:layout_height="24dp"
                android:inputType="textPersonName"
                android:text="Name"
                android:ems="10"
                android:id="@+id/RegisterNameId"
                android:textSize="18sp" app:layout_constraintTop_toTopOf="parent" android:layout_marginEnd="8dp"
                app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="8dp"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="264dp"
                app:layout_constraintHorizontal_bias="0.496"/>
        <EditText
                android:layout_width="264dp"
                android:layout_height="43dp"
                android:inputType="textPersonName"
                android:ems="10"
                android:id="@+id/RegisterNaamId" android:layout_marginTop="20dp"
                app:layout_constraintTop_toBottomOf="@+id/RegisterNameId"
                app:layout_constraintStart_toStartOf="@+id/RegisterNameId"
                app:layout_constraintEnd_toEndOf="@+id/RegisterNameId"/>
        <TextView
                android:layout_width="264dp"
                android:layout_height="24dp"
                android:inputType="textPersonName"
                android:text="Password"
                android:ems="10"
                android:id="@+id/registerNameId"
                android:textSize="18sp"
                app:layout_constraintTop_toBottomOf="@+id/RegisterNaamId"
                android:layout_marginTop="28dp"
                app:layout_constraintEnd_toEndOf="@+id/RegisterNaamId"
                app:layout_constraintStart_toStartOf="@+id/RegisterNaamId"/>
        <EditText
                android:layout_width="264dp"
                android:layout_height="wrap_content"
                android:inputType="textPassword"
                android:ems="10"
                android:id="@+id/editText2"
                app:layout_constraintEnd_toEndOf="@+id/registerNameId"
                app:layout_constraintStart_toStartOf="@+id/registerNameId" android:layout_marginTop="20dp"
                app:layout_constraintTop_toBottomOf="@+id/registerNameId" app:layout_constraintHorizontal_bias="1.0"/>
        <Button
                android:text="Register"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/button" app:layout_constraintEnd_toEndOf="@+id/editText2"
                app:layout_constraintStart_toStartOf="@+id/editText2" android:layout_marginTop="20dp"
                app:layout_constraintTop_toBottomOf="@+id/editText2"/>
        <TextView
                android:text="Switch to Login"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/RegisterSwapTextId" android:layout_marginTop="20dp"
                app:layout_constraintTop_toBottomOf="@+id/button" app:layout_constraintEnd_toEndOf="@+id/button"
                app:layout_constraintStart_toStartOf="@+id/button" app:layout_constraintHorizontal_bias="0.517"
                android:clickable="true" android:onClick="onClick"/>

    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>