<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Fragments.LoginFragment" android:id="@+id/frameLayout">
    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        <TextView
                android:layout_width="264dp"
                android:layout_height="24dp"
                android:inputType="textPersonName"
                android:text="Name"
                android:ems="10"
                android:id="@+id/LoginNaamId"
                android:layout_marginEnd="8dp"
                app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="8dp"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="264dp"
                app:layout_constraintTop_toTopOf="parent"
                android:textSize="18sp" app:layout_constraintHorizontal_bias="0.496"/>
        <TextView
                android:layout_width="264dp"
                android:layout_height="24dp"
                android:inputType="textPersonName"
                android:text="Password"
                android:ems="10"
                android:id="@+id/LoginPassWoord"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="@+id/editText8"
                app:layout_constraintStart_toStartOf="@+id/editText8" android:layout_marginTop="28dp"
                app:layout_constraintTop_toBottomOf="@+id/editText8" app:layout_constraintHorizontal_bias="0.0"/>
        <EditText
                android:layout_width="264dp"
                android:layout_height="wrap_content"
                android:inputType="textPassword"
                android:ems="10"
                android:id="@+id/editText7"
                app:layout_constraintStart_toStartOf="@+id/LoginPassWoord"
                app:layout_constraintEnd_toEndOf="@+id/LoginPassWoord" app:layout_constraintHorizontal_bias="0.0"
                android:layout_marginTop="20dp" app:layout_constraintTop_toBottomOf="@+id/LoginPassWoord"/>
        <EditText
                android:layout_width="264dp"
                android:layout_height="43dp"
                android:inputType="textPersonName"
                android:ems="10"
                android:id="@+id/editText8"
                app:layout_constraintStart_toStartOf="@+id/LoginNaamId"
                app:layout_constraintEnd_toEndOf="@+id/LoginNaamId" android:layout_marginTop="20dp"
                app:layout_constraintTop_toBottomOf="@+id/LoginNaamId" app:layout_constraintHorizontal_bias="0.0"/>
        <Button
                android:text="Login"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/LoginButton"
                app:layout_constraintStart_toStartOf="@+id/editText7"
                android:layout_marginTop="20dp"
                app:layout_constraintTop_toBottomOf="@+id/editText7" app:layout_constraintEnd_toEndOf="@+id/editText7"
                android:onClick="loginButtonClicked"/>
        <TextView
                android:text="Register an account"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/LoginSwapTextId" android:layout_marginTop="24dp"
                app:layout_constraintTop_toBottomOf="@+id/LoginButton"
                app:layout_constraintEnd_toEndOf="@+id/LoginButton"
                app:layout_constraintStart_toStartOf="@+id/LoginButton" app:layout_constraintHorizontal_bias="0.517"
                android:clickable="true"
                android:onClick="onClick"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>